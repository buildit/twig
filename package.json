    "nsp": "^3.2.1",
    "pre-commit": "1.2.2",
    "pre-push": "0.1.1",
    "protractor": "5.2.2",
    "protractor-jasmine2-screenshot-reporter": "0.4.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.9.5",
    "sass-lint": "~1.11.1",
    "ts-node": "3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~3.2.4",
    "webdriver-manager": "12.0.6"
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "build:prod",
    "test:ci",
    "test:e2e"
  ]
