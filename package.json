{
  "name": "twig",
  "version": "3.4.0",
  "license": "Apache-2.0",
  "angular-cli": {},
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --progress=false --prod",
    "serve": "ng serve",
    "lint": "npm run tslint && npm run sasslint",
    "tslint": "tslint \"src/**/*.ts\"",
    "sasslint": "sass-lint -v",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --progress=false --watch=false --code-coverage",
    "nsp": "nsp check",
    "start": "npm run serve",
    "pree2e": "webdriver-manager update --versions.chrome 2.36",
    "test:e2e": "ng e2e --port=4201",
    "test:e2e:ci": "ng e2e --serve false --webdriver-update=false",
    "version": "git add package.json",
    "postversion": "git push --no-verify && git push origin HEAD:master --no-verify && git push --tags --no-verify",
    "validate": "npm ls"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.4",
    "@angular/common": "~7.2.4",
    "@angular/compiler": "~7.2.4",
    "@angular/core": "~7.2.4",
    "@angular/forms": "~7.2.4",
    "@angular/http": "~7.2.4",
    "@angular/platform-browser": "~7.2.4",
    "@angular/platform-browser-dynamic": "~7.2.4",
    "@angular/platform-server": "~7.2.4",
    "@angular/router": "~7.2.4",
    "@angular/service-worker": "^7.2.11",
    "@ng-bootstrap/ng-bootstrap": "4.0.3",
    "angular-bound-sensor": "^1.1.0",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "4.0.0-beta",
    "bound-sensor": "^1.1.0",
    "core-js": "^2.5.3",
    "d3-ng2-service": "^2.2.0",
    "dragula": "^3.7.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "immutable": "^3.8.1",
    "markdown-to-html-pipe": "^1.2.1",
    "marked": "^0.3.17",
    "moment": "^2.18.1",
    "ng-trim-value-accessor": "^2.0.4",
    "ng2-dragula": "^1.5.0",
    "ng2-select": "^2.0.0",
    "ngx-toastr": "^9.1.2",
    "ramda": "^0.24.1",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "ts-helpers": "^1.1.2",
    "tslib": "^1.9.0",
    "valid-url": "^1.0.9",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
